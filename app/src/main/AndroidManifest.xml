<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xstar.com.commons" >

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application android:theme="@style/AppTheme" >
        <activity
            android:name=".LauncherActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ButtonToBiggerActivity"
            android:label="@string/title_activity_button_to_bigger" >
        </activity>
        <activity
            android:name=".MenuListActivity"
            android:label="@string/title_activity_menu_list" >
        </activity>
        <activity
            android:name=".DialogFragmentActivity"
            android:label="@string/title_activity_dialog_fragment" >
        </activity>
        <activity
            android:name=".DialogActivity"
            android:theme="@style/MyDialogStyle" />
        <activity
            android:name=".TwoDGenereteActivity"
            android:label="@string/title_activity_two_dscan" >
        </activity>
        <activity
            android:name=".CameraActivity"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name=".DataShowActivity"
            android:label="@string/title_activity_data_show" >
        </activity>
    </application>

</manifest>
